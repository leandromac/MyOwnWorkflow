<p id="notice"><%= notice %></p>

<h1>Listing Members</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Degree</th>
      <th>Location</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Portfolio</th>
      <th>Skill</th>
      <th>Feedback</th>
      <th>Deadline</th>
      <th>Quality</th>
      <th>Others skills</th>
      <th>Note</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.name %></td>
        <td><%= member.degree %></td>
        <td><%= member.location %></td>
        <td><%= member.email %></td>
        <td><%= member.phone %></td>
        <td><%= member.portfolio %></td>
        <td><%= member.skill %></td>
        <td><%= member.feedback %></td>
        <td><%= member.deadline %></td>
        <td><%= member.quality %></td>
        <td><%= member.others_skills %></td>
        <td><%= member.note %></td>
        <td><%= member.image %></td>
        <td><%= link_to 'Show', member %></td>
        <td><%= link_to 'Edit', edit_member_path(member) %></td>
        <td><%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member', new_member_path %>
